# Test Environment Configuration
NODE_ENV=test
PORT=3003

# Database
DATABASE_TYPE=sqlite
DATABASE_URL=:memory:

# JWT
JWT_SECRET=test-secret-key-for-e2e-tests
JWT_EXPIRES_IN=1h

# File Storage
STORAGE_PATH=/tmp/test-videos
UPLOAD_PATH=/tmp/test-uploads

# Video Processing
FFMPEG_PATH=ffmpeg
TEMP_DIR=/tmp/test-processing

# Queue
QUEUE_NAME=test-video-processing

# Logging
LOG_LEVEL=error
